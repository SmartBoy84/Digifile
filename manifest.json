{
	"manifest_version": 3,
	"name": "Digiscrape",
	"version": "1.0.0",
	"permissions": [
		"nativeMessaging",
		"tabs",
		"webRequest",
		"downloads",
		"scripting"
	],
	"host_permissions": [
		"https://digify.com/*"
	],
	"description": "attempt at breaking digify ig",
	"content_scripts": [
		{
			"matches": [
				"https://digify.com/*"
			],
			"js": [
				"downloader/receiver.js",
				"downloader/helper.js",
				"downloader/snapper.js",
				"downloader/jspdf.min.js"
			],
			"run_at": "document_start"
		}
	],
	"action": {
		"default_popup": "input/input.html"
	},
	"background": {
		"service_worker": "scraper/scraper.js"
	}
}